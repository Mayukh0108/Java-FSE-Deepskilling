c3f0ae9b-79fe-4075-83c1-417d0a0a6db3:1750954574953:164:S:CREATE TABLE Customers (\n    CustomerID     NUMBER PRIMARY KEY,\n    Name           VARCHAR2(100),\n    Age            NUMBER,\n    Balance        NUMBER(10,2),\n    InterestRate   NUMBER(5,2),\n    IsVIP          VARCHAR2(5) DEFAULT 'FALSE'\n);
